/**app.wxss**/

@font-face {
  font-family: 'webfont';
  src: url('//at.alicdn.com/t/webfont_uoime7ejox9.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_uoime7ejox9.eot?#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('//at.alicdn.com/t/webfont_uoime7ejox9.woff') format('woff'),
         /* chrome、firefox */ url('//at.alicdn.com/t/webfont_uoime7ejox9.ttf') format('truetype'),
         /* chrome、firefox、opera、Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/webfont_uoime7ejox9.svg#杨任东竹石体-Bold') format('svg'); /* iOS 4.1- */
}

.web-font {
  font-family: "webfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
